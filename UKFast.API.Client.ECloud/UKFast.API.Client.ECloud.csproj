<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>UKFast</Authors>
    <Company>UKFast</Company>
    <Description>Client for interacting with UKFast eCloud</Description>
    <PackageLicenseUrl>https://raw.githubusercontent.com/ukfast/sdk-dotnet-ecloud/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/ukfast/sdk-dotnet-ecloud</PackageProjectUrl>
    <PackageTags>sdk, client, ukfast, ecloud</PackageTags>
    <Version>1.0.10</Version>
    <AssemblyVersion>1.0.10.0</AssemblyVersion>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="UKFast.API.Client.Core" Version="1.0.4" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Models\V2\" />
  </ItemGroup>

</Project>
